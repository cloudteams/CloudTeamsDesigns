{
  "name": "cloudteams-frontend",
  "version": "1.0.0",
  "description": "Frontend files for the CloudTeams website",
  "directories": {
    "doc": "doc"
  },
  "scripts": {
    "preclean": "mkdirp ./tmp",
    "clean": "rimraf ./dist/* && rimraf ./tmp/*",
    "predebug": "npm run clean",
    "debug": "set NODE_ENV=debug && webpack --debug",
    "postdebug": "npm run sync",
    "preproduction": "npm run clean",
    "production": "set NODE_ENV=production && webpack -p --progress",
    "postproduction": "npm run sync",
    "sync": "sync-files ./dist `json -f package.user.json sync-dir`",
    "prewatch": "npm run clean",
    "watch": "set NODE_ENV=debug && webpack --watch",
    "onbuild": "npm run sync"
  },
  "author": "Booreiland",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "css-loader": "^0.23.1",
    "expose-loader": "^0.7.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.1",
    "image-webpack-loader": "^1.4.0",
    "imagemin": "^4.0.0",
    "node-sass": "^3.2.0",
    "on-build-webpack": "^0.1.0",
    "rimraf": "^2.4.3",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "sync-files": "^1.0.3",
    "url-loader": "^0.5.5",
    "webpack": "^1.12.0",
    "webpack-path-rewriter": "^1.1.2"
  }
}
