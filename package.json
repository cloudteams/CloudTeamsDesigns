{
  "name": "cloudteams-frontend",
  "version": "1.0.0",
  "description": "Frontend files for the CloudTeams website",
  "repository": {
    "type": "git",
    "url": "git://github.com/cloudteams/designs.git"
  },
  "directories": {
    "doc": "doc"
  },
  "scripts": {
    "preclean": "mkdirp ./tmp",
    "clean": "rimraf ./dist/* && rimraf ./dist/.[!.]* && rimraf ./tmp/*",
    "predebug": "npm run clean",
    "debug": "NODE_ENV=debug webpack -d --debug",
    "postdebug": "npm run sync",
    "preproduction": "npm run clean",
    "production": "NODE_ENV=production webpack -p --progress",
    "postproduction": "npm run sync",
    "sync": "sync-files ./dist `json -f package.user.json sync-dir`",
    "prewatch": "npm run clean",
    "watch": "NODE_ENV=debug webpack -d --watch",
    "onbuild": "npm run sync",
    "upload": "ftpsync"
  },
  "author": "Booreiland",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.3.1",
    "babel-core": "^6.5.1",
    "babel-loader": "^6.2.2",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "copy-webpack-plugin": "^1.1.1",
    "css-loader": "^0.23.1",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "glob": "^6.0.4",
    "image-webpack-loader": "^1.6.3",
    "imagemin": "^4.0.0",
    "node-sass": "^3.4.2",
    "on-build-webpack": "^0.1.0",
    "postcss-loader": "^0.8.0",
    "purifycss-webpack-plugin": "^2.0.1",
    "sass-loader": "^3.1.2",
    "source-list-map": "^0.1.5",
    "source-map": "^0.5.3",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.13",
    "webpack-path-rewriter": "^1.1.3"
  }
}
